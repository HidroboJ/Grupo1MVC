@model List<CursoModel>
@{
    ViewData["Title"] = "Multi Curso";
    Layout = "Simple";
}


<table class="table table-dark  table-hover">
    <thead>
        <tr>
            <th scope="col">
                Nombre
            </th>
            <th scope="col">
                ID
            </th>
            <th scope="col">
                Jornada
            </th>
            <th scope="col">
                Dirección
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var curso in Model)
        { 
        <tr>
            <td>
               @curso.Nombre 
            </td>
            <td>
                @curso.Id
            </td>
             <td>
                @curso.Jornada
            </td>
                <td>
                    @curso.Dirección
                </td>
        </tr>
        }
    </tbody>
</table>


<h2><strong>@ViewBag.DatoDinamico</strong></h2>