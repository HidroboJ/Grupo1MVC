
@model List<AlumnoModel>
@{
    ViewData["Title"] = "Multi Alumno";
    Layout = "Simple";
}

<table class="table table-dark  table-hover">
    <thead>

        <tr>
            <th scope="col">
                Nombre
            </th>
            <th scope="col">
                ID
            </th>
            <th scope="col">
                IDCURSO
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var alumno in Model)
        {
            <tr>
                <td>
                    @alumno.Nombre
                </td>
                <td>
                    @alumno.Id
                </td>
                <td>
                    @alumno.CursoId
                </td>
            </tr>
        }
    </tbody>
</table>


<h2><strong>@ViewBag.DatoDinamico</strong></h2>